set folder=$HOME/mail/mgweb.de
set mbox=$HOME/mail/mgweb.de/
set spoolfile=+INBOX
set record = +Gesendet
set postponed= +Entwurf
unmailboxes *
mailboxes = `~/bin/sidebar_config_from_maildir.py mgweb.de`
set realname = 'Mattias Giese'
